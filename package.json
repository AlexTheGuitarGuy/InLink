  "proxy": "https://social-network.samuraijs.com/api/1.0/",
  "scripts": {
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lintCheck": "eslint --ext .js,.jsx,.ts,.tsx . --max-warnings 0",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
